<header class="header">
    {% block logo %}
        {% set brand_path = (admingenerator_config('dashboard_welcome_path') is not null) ? path(admingenerator_config('dashboard_welcome_path')) : '#' %}
        <a class="logo" href="{{ brand_path }}">
            {% block brand_text %}
                {{ 'dashboard.brand'|trans({}, 'Admingenerator') }}
            {% endblock %}
        </a>
    {% endblock %}
    {% block navbar %}
        {% include "AdmingeneratorGeneratorBundle:navbar:layout.html.twig" %}
    {% endblock navbar %}
</header>