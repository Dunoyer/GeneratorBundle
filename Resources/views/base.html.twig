{% extends "AdmingeneratorGeneratorBundle::base_uncompressed.html.twig" %}

{% block stylesheets %}
    {% stylesheets package='admingenerator_css' filter='cssrewrite,?uglifycss'
        output='assetic/css/compiled/admingenerator.min.css'
        'components/bootstrap/dist/css/bootstrap.css'
        'components/font-awesome/css/font-awesome.css'
        'components/iCheck/skins/minimal/_all.css'
        'components/adminlte/css/AdminLTE.css'
        '@AdmingeneratorGeneratorBundle/Resources/public/css/main.css'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="all" />
    {% endstylesheets %}
{% endblock %}

{% block scripts_head %}
    {% javascripts package='admingenerator_js' filter='?uglifyjs2'
    output='assetic/js/compiled/ie9.min.js'
        'components/html5shiv/dist/html5shiv.min.js'
        'components/respond/dest/respond.min.js'
    %}
    <!--[if lt IE 9]>
        <script type="text/javascript" src="{{ asset_url }}"></script>
    <![endif]-->
    {% endjavascripts %}
{% endblock %}

{% block javascripts %}
    {% javascripts package='admingenerator_js' filter='?uglifyjs2'
    output='assetic/js/compiled/admingenerator.min.js'
        'components/jquery/jquery.js'
        'components/bootstrap/dist/js/bootstrap.js'
        'components/iCheck/icheck.js'
        'components/slimScroll/jquery.slimscroll.js'
        'components/jquery-resize/jquery.ba-resize.js'
        '@AdmingeneratorGeneratorBundle/Resources/public/js/adminlte.js'
        '@AdmingeneratorGeneratorBundle/Resources/public/js/main.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}