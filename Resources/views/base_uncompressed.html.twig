<!DOCTYPE html>
<html>
<head>
    {% block html_head_tag_inner %}
    <meta charset="utf-8">
    <title>{% block title %}Admin{% endblock %}</title>
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('components/bootstrap/css/bootstrap.min.css') }}" type="text/css" media="all"/>
        <link rel="stylesheet" href="{{ asset('components/font-awesome/css/font-awesome.min.css') }}" type="text/css" media="all"/>
        <link rel="stylesheet" href="{{ asset('components/onicons/css/iconicons.min.css') }}" type="text/css" media="all"/>
        <link rel="stylesheet" href="{{ asset('components/adminlte/css/AdminLTE.css') }}" type="text/css" media="all"/>
    {% endblock %}

    {% block icons %}
        <!-- Favicon and touch icons -->
        <link rel="shortcut icon" href="{{ asset('bundles/admingeneratorgenerator/ico/favicon.ico') }}">
        <link rel="apple-touch-icon-precomposed" sizes="144x144"
              href="{{ asset('bundles/admingeneratorgenerator/ico/apple-touch-icon-144-precomposed.png') }}">
        <link rel="apple-touch-icon-precomposed" sizes="114x114"
              href="{{ asset('bundles/admingeneratorgenerator/ico/apple-touch-icon-114-precomposed.png') }}">
        <link rel="apple-touch-icon-precomposed" sizes="72x72"
              href="{{ asset('bundles/admingeneratorgenerator/ico/apple-touch-icon-72-precomposed.png') }}">
        <link rel="apple-touch-icon-precomposed"
              href="{{ asset('bundles/admingeneratorgenerator/ico/apple-touch-icon-57-precomposed.png') }}">
    {% endblock %}

    {% block scripts_head %}
    {% endblock %}
    {% endblock html_head_tag_inner %}
</head>


{% block html_body_tag %}
<body class="skin-blue">
{% endblock html_body_tag %}
{% block html_body_tag_inner %}

{% block header %}
    {% include 'AdmingeneratorGeneratorBundle::header.twig' %}
{% endblock header%}

<div class="container-fluid">
    {% block flashes %}
        <div id="flashes" class="row-fluid hidden-print">
            {% for type, flashMessages in app.session.flashbag.all() %}
                {% set alert = type is sameas('error') ? 'danger' : type %}
                {% for flashMessage in flashMessages %}
                    <div class="alert alert-{{ alert }}">
                        <button type="button" class="close" data-dismiss="alert">&times;</button>
                        {{ flashMessage|raw }}
                    </div>
                {% endfor %}
            {% endfor %}
        </div><!--/.row -->
    {% endblock %}

    {% block body %}
        <div class="row-fluid">
        </div><!--/.row -->
    {% endblock %}

    {% block footer %}
        <footer class="hidden-print">
            <hr>
            <p>{% block footer_text %}&copy;
                Symfony2Admingenerator {{ ("now"|date('Y') > 2012) ? '2012 - ' ~ "now"|date('Y') : '2012' }}{% endblock %}</p>
        </footer>
    {% endblock %}
</div>
<!--/.container-fluid -->

{% block javascripts %}
    <script type="text/javascript" src="{{ asset('components/bootstrap/js/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/admingeneratorgenerator/js/iScroll/iScroll.js') }}"></script>
    <script type="text/javascript"
            src="{{ asset('bundles/admingeneratorgenerator/js/admingenerator/general.js') }}"></script>
    <script type="text/javascript"
            src="{{ asset('bundles/admingeneratorgenerator/js/admingenerator/tabs.js') }}"></script>
    <script type="text/javascript"
            src="{{ asset('bundles/admingeneratorgenerator/js/admingenerator/batch-actions.js') }}"></script>
    <script type="text/javascript"
            src="{{ asset('bundles/admingeneratorgenerator/js/admingenerator/generic-actions.js') }}"></script>
    <script type="text/javascript"
            src="{{ asset('bundles/admingeneratorgenerator/js/admingenerator/object-actions.js') }}"></script>
    <script type="text/javascript"
            src="{{ asset('bundles/admingeneratorgenerator/js/treeTable/jquery.treeTable-extended.js') }}"></script>
    <script type="text/javascript"
            src="{{ asset('bundles/admingeneratorgenerator/js/admingenerator/nestedset.js') }}"></script>
{% endblock %}

{% endblock html_body_tag_inner %}
</body>
</html>
